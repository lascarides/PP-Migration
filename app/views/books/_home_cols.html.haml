.row
	.col-md-6.date-column
		%h3= link_to "By Date", "/#{@type.to_s.dasherize}/date"

		%ul
			- 12.times do |d|
				- decade = (d * 10) + 1830
				%li= link_to "#{decade}s", "/#{@type.to_s.dasherize}/date/#{decade}s"
				
	.col-md-6
		%h3= link_to "By subject", "/#{@type.to_s.dasherize}/title"

		%ul
			- 10.times do 
				%li Coming sooon
